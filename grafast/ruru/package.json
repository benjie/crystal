{
  "name": "@grafast/ruru",
  "version": "0.0.0",
  "description": "Grafast-flavoured GraphiQL distribution",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./dist/cli-run.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "node": "./dist/server.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "node": "./dist/cli.js"
    }
  },
  "scripts": {
    "webpack": "node --loader ts-node/esm node_modules/.bin/webpack-cli",
    "watch": "yarn webpack --watch --mode=development",
    "prepack": "rm -Rf dist bundle tsconfig.tsbuildinfo && tsc -b && yarn webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphile/postgraphile.git"
  },
  "keywords": [
    "graphile",
    "graphql",
    "graphiql"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/postgraphile/issues"
  },
  "homepage": "https://grafast.org/ruru/",
  "dependencies": {
    "@graphiql/plugin-explorer": "^0.1.5",
    "@graphiql/react": "^0.13.2",
    "@graphiql/toolkit": "^0.8.0",
    "@types/node": "^16.11.15",
    "@types/yargs": "^17.0.10",
    "graphile-config": "workspace:^",
    "graphiql": "^2.0.8",
    "graphiql-explorer": "^0.9.0",
    "react": ">=16 <19",
    "react-dom": ">=16 <19",
    "tslib": "^2.4.0",
    "yargs": "^17.4.1"
  },
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "files": [
    "dist",
    "bundle",
    "index.js"
  ],
  "devDependencies": {
    "@types/codemirror": "^5.60.5",
    "@types/debug": "^4.1.7",
    "@types/http-proxy": "^1",
    "@types/jest": "^27.5.1",
    "@types/nodemon": "1.19.1",
    "@types/react": "^18",
    "@types/react-dom": "^18.0.0",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "graphql": "16.1.0-experimental-stream-defer.6",
    "http-proxy": "^1.18.1",
    "jest": "^28.1.0",
    "nodemon": "^2.0.15",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.1-rc",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
