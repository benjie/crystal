{
  "name": "postgraphile",
  "version": "5.0.0-alpha.0",
  "description": "Automatic, high performance, and highly customizable GraphQL API for PostgreSQL",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "node": "./dist/cli.js"
    },
    "./preset/amber": {
      "types": "./dist/preset/amber.d.ts",
      "node": "./dist/preset/amber.js"
    },
    "./preset/v4": {
      "types": "./dist/preset/v4.d.ts",
      "node": "./dist/preset/v4.js"
    }
  },
  "bin": "./dist/cli-run.js",
  "scripts": {
    "test": "jest -i",
    "prepack": "tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphile/graphile-engine.git"
  },
  "keywords": [
    "graphile",
    "graphql",
    "engine",
    "postgraphile",
    "pg",
    "postgres",
    "postgresql",
    "graphqljs",
    "plugin",
    "engine",
    "extension"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/graphile-engine/issues"
  },
  "homepage": "https://graphile.org/graphile-engine/",
  "dependencies": {
    "@graphile/lru": "4.6.0-alpha.0",
    "@types/node": "^16.11.15",
    "@types/pg": "^8.6.2",
    "dataplanner": "workspace:^",
    "debug": "^4.3.3",
    "graphile-build": "workspace:*",
    "graphile-build-pg": "workspace:*",
    "graphile-config": "workspace:^",
    "graphile-export": "workspace:^",
    "graphile-inspect": "workspace:^",
    "graphql": "16.1.0-experimental-stream-defer.6",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "tslib": "^2.3.1"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8",
    "@types/nodemon": "1.19.1",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "rc"
  }
}
