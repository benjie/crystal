{
  "name": "graphile-export",
  "version": "0.0.1",
  "description": "Take a GraphQL schema from memory and write it to file",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "default": "./dist/helpers.js"
    }
  },
  "scripts": {
    "test": "jest -i",
    "prepack": "tsc -b"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphile/graphile-build.git"
  },
  "keywords": [
    "graphql",
    "schema",
    "memory",
    "export",
    "output",
    "write",
    "file",
    "graphile"
  ],
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graphile/graphile-build/issues"
  },
  "homepage": "https://graphile.org/",
  "dependencies": {
    "@babel/generator": "^7.16.5",
    "@babel/parser": "^7.16.6",
    "@babel/template": "^7.16.0",
    "@types/node": "^16.11.15",
    "dataplanner": "workspace:^",
    "prettier": "^2.5.1",
    "tslib": "^2.3.1"
  },
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "graphql": "*",
    "pg-sql2": "*"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/babel__generator": "^7.6.3",
    "@types/babel__template": "^7.4.1",
    "@types/jest": "^27.0.3",
    "@types/prettier": "^2",
    "graphql": "16.1.0-experimental-stream-defer.6",
    "jest": "^27.4.5",
    "ts-node": "^10.4.0",
    "typescript": "rc"
  }
}
